generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            String          @id @default(uuid())
  ShareInstance ShareInstance[]
}

model ShareInstance {
  id         String @id @default(uuid())
  data       String @default("{}")
  dbName     String? @unique
  fromUser   User?   @relation(fields: [fromUserId], references: [id])
  fromUserId String?
  updatedAt DateTime @updatedAt
}
